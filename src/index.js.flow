// @flow
/* eslint-disable no-redeclare */

import type { Node, ComponentType } from 'react';

type Dependency = Function;

declare export class DiProvider extends React$Component<
  {
    use: Dependency[],
    children: Node,
  },
  {
    getDependencies: (deps: Dependency[]) => Dependency[],
  }
> {}

declare export function withDi(
  component: ComponentType<any>,
  dependencies: Dependency[]
): void;

declare export function di(...dependencies: Dependency[]): void;

declare export function diMock<T: Dependency>(original: T, mock: T): T;
